        <Declaration><![CDATA[VAR
END_VAR
]]></Declaration>
        <Implementation>
          <ST><![CDATA[ButtonAuto :=  _buttonAuto._signal;]]></ST>
        </Implementation>
      </Get>
      <Set Name="Set" Id="{7c081f7a-9837-49d7-8576-bec7571aaf4f}">
        <Declaration><![CDATA[VAR
END_VAR
]]></Declaration>
        <Implementation>
          <ST><![CDATA[// Light up illumination of button
IF ButtonAuto THEN
	_buttonAutoLight.Set();
ELSE
	_buttonAutoLight.Reset();
END_IF]]></ST>
        </Implementation>
      </Set>
    </Property>
